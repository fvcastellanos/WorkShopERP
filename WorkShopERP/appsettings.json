{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MySql": {
    "Client": {
      "Server": "${MYSQL_HOST}",
      "Port": "${MYSQL_PORT}",
      "Username": "${WORKSHOP_USER}",
      "Password": "${WORKSHOP_PWD}",
      "Database": "${WORKSHOP_DB}"
    }
  },
  "AllowedHosts": "*",
  "Auth0": {
    "Domain": "${AUTH0_DOMAIN}",
    "ClientId": "${AUTH0_CLIENT_ID}"
  }
}
