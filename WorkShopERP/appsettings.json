{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MySql": {
    "Client": {
      "Server": "${MYSQL_HOST?mysql-host}",
      "Port": "${MYSQL_PORT?3306}",
      "Username": "${WORKSHOP_USER?workshop}",
      "Password": "${WORKSHOP_PWD?workshop}",
      "Database": "${WORKSHOP_DB?workshop}"
    }
  },
  "AllowedHosts": "*"
}
